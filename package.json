{
  "name": "treeshake-example",
  "version": "1.0.0",
  "type": "module",
  "source": "src/index.ts",
  "scripts": {
    "build": "npm run clean && npm run build:ncc && npm run build:rollup && npm run build:webpack",
    "clean": "rm -rf dist",
