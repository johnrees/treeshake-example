    "build:rollup": "rollup -c",
    "build:webpack": "webpack"
  },
  "dependencies": {
    "@solana/web3.js": "^1.29.2"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.31.1",
    "esbuild": "^0.13.5",
    "rollup": "^2.58.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0"
  }
}
